<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 5.3//EN" "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">

<hibernate-mapping>
  <class name="javaandhibernate_app.members" table="demo">
  <id name="id">
      <generator class="assigned"></generator>
  </id>
  
  <property name="firstname"></property>
  <property name="lastname"></property>
  <property name="mobile"></property>
  <property name="email"></property>
  <property name="password"></property>
</class>  
</hibernate-mapping>

